   "source": [
    "out_video_path = 'sit-stand-sample-out.mp4'\n",
    "\n",
    "video_cap = cv2.VideoCapture(0)\n",
    "video_cap.set(cv2.CAP_PROP_FRAME_WIDTH, 1024)\n",
    "video_cap.set(cv2.CAP_PROP_FRAME_HEIGHT, 768)\n",
    "video_width = 1024\n",
    "video_height = 768\n",
    "video_fps = video_cap.get(cv2.CAP_PROP_FPS)\n",
    "sit_count = 0\n",
    "stand_count = 0\n",
    "with mp_pose.Pose(\n",
    "    min_detection_confidence=0.5,\n",
    "    min_tracking_confidence=0.5) as pose:\n",
    "    while True:\n",
    "        success, raw_image = video_cap.read()\n",
    "        if not success:\n",
    "            print(\"Ignoring empty camera frame.\")\n",
    "            continue\n",
    "        image = raw_image\n",
    "        image.flags.writeable = False\n",
    "        image = cv2.cvtColor(image, cv2.COLOR_BGR2RGB)\n",
    "        results = pose.process(image)\n",
    "        # Draw the pose annotation on the image.\n",
    "        image.flags.writeable = True\n",
    "        image = cv2.cvtColor(image, cv2.COLOR_RGB2BGR)\n",
    "        mp_drawing.draw_landmarks(\n",
    "            image,\n",
    "            results.pose_landmarks,\n",
    "            mp_pose.POSE_CONNECTIONS,\n",
    "            landmark_drawing_spec=mp_drawing_styles.get_default_pose_landmarks_style())\n",
    "        if sit_count<5:\n",
    "            cv2.putText(image,\"press p to take sitting photos\",(500,600),cv2.FONT_HERSHEY_SIMPLEX,1,(255,0,0))\n",
    "        # Flip the image horizontally for a selfie-view display.\n",
    "            cv2.imshow('MediaPipe Pose', image)\n",
    "            if cv2.waitKey(25) & 0xFF == ord(\"p\"):\n",
    "                sit_count +=1\n",
    "                cv2.imwrite(\"fitness_poses_images_in/sit/%d.jpg\"%sit_count,raw_image)\n",
    "        else:\n",
    "            cv2.putText(image,\"press p to take standing photos\",(500,600),cv2.FONT_HERSHEY_SIMPLEX,1,(255,0,0))\n",
    "        # Flip the image horizontally for a selfie-view display.\n",
    "            cv2.imshow('MediaPipe Pose', image)\n",
    "\n",
    "            if cv2.waitKey(25) & 0xFF == ord(\"p\"):\n",
    "                stand_count +=1\n",
    "                cv2.imwrite(\"fitness_poses_images_in/stand/%d.jpg\"%stand_count,raw_image)\n",
    "            if stand_count==5:\n",
    "                break\n",
    "                \n",
    "cv2.destroyAllWindows()\n",
    "\n",
    "bootstrap_images_in_folder = 'fitness_poses_images_in'\n",
    "# Output folders for bootstrapped images and CSVs.\n",
    "bootstrap_images_out_folder = 'fitness_poses_images_out'\n",
    "bootstrap_csvs_out_folder = 'fitness_poses_csvs_out'\n",
    "\n",
    "# Initialize helper.\n",
    "bootstrap_helper = BootstrapHelper(\n",
    "    images_in_folder=bootstrap_images_in_folder,\n",
    "    images_out_folder=bootstrap_images_out_folder,\n",
    "    csvs_out_folder=bootstrap_csvs_out_folder,\n",
    ")\n",
    "# Check how many pose classes and images for them are available.\n",
    "bootstrap_helper.print_images_in_statistics()\n",
    "# Bootstrap all images.\n",
    "# Set limit to some small number for debug.\n",
    "bootstrap_helper.bootstrap()\n",
    "# Check how many images were bootstrapped.\n",
    "# After initial bootstrapping images without detected poses were still saved in\n",
    "# the folderd (but not in the CSVs) for debug purpose. Let's remove them.\n",
    "bootstrap_helper.align_images_and_csvs(print_removed_items=False)\n",
    "bootstrap_helper.print_images_out_statistics()\n",
    "dump_for_the_app()\n",
    "# Open output video.\n",
    "out_video = cv2.VideoWriter(out_video_path, cv2.VideoWriter_fourcc(*'mp4v'), video_fps, (video_width, video_height))\n",
    "frame_idx = 0\n",
